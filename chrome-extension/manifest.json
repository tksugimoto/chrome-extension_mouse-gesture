{
	"name": "Mouse gesture",
	"version": "0.1.0",
	"manifest_version": 3,
	"description": "マウスジェスチャー機能追加: 右クリックメニューを押しながら左右で戻る/進む, 上でタブを閉じる",
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [{
		"run_at": "document_start",
		"matches": [
			"file:///*",
			"*://*/*"
		],
		"js": [
			"gesture.js"
		],
		"all_frames": true
	}]
}
